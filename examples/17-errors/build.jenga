
# This example is a baseline for testing the reporting various build errors

alpha: beta gamma
  echo '[' $$(cat $ins) ']' > $out

beta : delta epsilon
  echo '[' $$(cat $ins) ']' > $out

gamma : zeta eta theta
  echo '[' $$(cat $ins) ']' > $out

delta : : echo D > $out
epsilon : : echo E > $out
zeta : : echo Z > $out
eta : : echo H > $out
theta : : echo T > $out
