
# This example is a baseline for testing the reporting various build errors

alpha: beta gamma
  echo '[' $$(cat $^) ']' > $@

beta : delta epsilon
  echo '[' $$(cat $^) ']' > $@

gamma : zeta eta theta
  echo '[' $$(cat $^) ']' > $@

delta : : echo D > $@
epsilon : : echo E > $@
zeta : : echo Z > $@
eta : : echo H > $@
theta : : echo T > $@
