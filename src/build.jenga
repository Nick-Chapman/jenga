
executable-name:
  echo jenga.exe > $out

include ../rulelib/haskell.jenga

main-entry:
  echo Engine > $out

ghc-version:
  echo 9.10.3 > $out

ghc-options:
  echo -Werror >> $out
  echo -Wall >> $out
  echo -Wno-name-shadowing >> $out

default-extensions:
  echo LambdaCase >> $out
  echo DuplicateRecordFields >> $out
  echo GADTs >> $out

packages:
  echo MissingH >> $out
  echo data-ordlist >> $out
  echo filelock >> $out
  echo optparse-applicative >> $out
  echo split >> $out
